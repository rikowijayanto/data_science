    "            \n",
    "            for tweet in new_tweets:\n",
    "                if(tweet.created_at.date() < datetime.now().date()) :\n",
    "                    f.write(jsonpickle.encode(tweet._json,unpicklable=False)+'\\n')\n",
    "            tweetCount+=len(new_tweets)\n",
